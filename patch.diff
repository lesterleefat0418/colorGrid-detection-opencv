+
+		void Update()
+		{
+			if (mainTexture == null)
+			{
+				return;
+			}
+
+			if (_setNativeSize)
+			{
+				SetNativeSize();
+			}
+			if (HasValidTexture())
+			{
+				if (mainTexture.width != _lastWidth ||
+					mainTexture.height != _lastHeight)
+				{
+					_lastWidth = mainTexture.width;
+					_lastHeight = mainTexture.height;
+					SetVerticesDirty();
+					SetMaterialDirty();
+				}
+			}
+
+			if (mainTexture != _lastTexture)
+			{
+				SetMaterialDirty();
+				_lastTexture = mainTexture;
+			}
