+		Pass
+		{
+			CGPROGRAM
+			#pragma vertex vert
+			#pragma fragment frag
+			#pragma multi_compile __ APPLY_GAMMA
+
+			#include "UnityCG.cginc"
+			#include "AVProLiveCamera_Shared.cginc"
