+				vert.position = new Vector2(v.x, v.w);
+				vert.uv0 = new Vector2(m_UVRect.xMin, m_UVRect.yMax);
+				vert.color = color;
+				vbo.Add(vert);
+
+				vert.position = new Vector2(v.z, v.w);
+				vert.uv0 = new Vector2(m_UVRect.xMax, m_UVRect.yMax);
+				vert.color = color;
+				vbo.Add(vert);
+
+				vert.position = new Vector2(v.z, v.y);
